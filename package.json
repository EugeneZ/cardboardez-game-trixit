{
  "name": "cardboardez",
  "description": "CardboardEZ is a tool that helps simulate various tabletop games.",
  "author": "Eugene Zaretskiy",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "npm run build && node server.js",
    "build": "browserify -t babelify -t require-globify client/main.js | uglifyjs - -c -m -o public/assets/scripts/client.js",
    "watchify": "watchify --debug -t babelify -t require-globify -p livereactload client/main.js -o public/assets/scripts/client.js",
    "nodemon": "nodemon --debug --ignore client/ --ignore public/ --ignore game/*/component/* server.js",
    "test": "tape -r babel-register \"test/unit/**/*.js\""
  },
  "dependencies": {
    "autobind-decorator": "^1.3.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-function-bind": "^6.8.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-latest": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.14.0",
    "babelify": "^7.3.0",
    "body-parser": "^1.10.2",
    "browserify": "^13.1.0",
    "compression": "^1.6.2",
    "config": "^1.10.0",
    "crypto-js": "^3.1.6",
    "feathers": "^2.0.2",
    "feathers-authentication": "^0.7.10",
    "feathers-errors": "^2.4.0",
    "feathers-hooks": "^1.5.7",
    "feathers-rest": "^1.5.0",
    "feathers-rethinkdb": "^0.2.3",
    "feathers-socketio": "^1.4.1",
    "hammerjs": "^2.0.8",
    "lodash": "^4.16.2",
    "material-ui": "^0.15.4",
    "moment": "^2.17.0",
    "passport-facebook": "^1.0.3",
    "passport-github": "^1.1.0",
    "passport-google-oauth20": "^1.0.0",
    "passport-vimeo-oauth2": "0.0.3",
    "react": "^15.3.2",
    "react-dnd": "^2.1.4",
    "react-dnd-html5-backend": "^2.1.2",
    "react-dnd-touch-backend": "^0.3.3",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.6.0",
    "redux-saga": "^0.11.1",
    "require-globify": "^1.4.1",
    "rethinkdbdash": "^2.3.23",
    "socket.io-client": "^1.4.8",
    "uglify-js": "^2.7.3"
  },
  "devDependencies": {
    "livereactload": "^3.1.1",
    "nodemon": "^1.10.2",
    "react-proxy": "^1.1.8",
    "tape": "^4.6.0",
    "watchify": "^3.7.0"
  }
}
